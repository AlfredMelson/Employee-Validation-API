{
  "name": "validation_api_dev",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Alfred Melson",
  "tags": [
    "employee-registration",
    "registration-validation"
  ],
  "keywords": [
    "bcrypt",
    "body-parser",
    "cookie-parser",
    "cors",
    "express",
    "express-validator",
    "jsonwebtoken",
    "uuid",
    "zod",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlfredMelson/validation_api_dev.git"
  },
  "main": "server.ts",
  "scripts": {
    "build-ts": "tsc",
    "build": "yarn build-ts && yarn lint-fx",
    "clean-svge": "rimraf coverage",
    "clean-build": "rimraf build",
    "clean-mdls": "lerna clean --yes && rimraf node_modules yarn.lock",
    "debug": "yarn build && yarn watch-debug",
    "dev": "tsnd --respawn --transpile-only server.ts",
    "lint-ck": "eslint --ext .ts",
    "lint-fx": "eslint --ext .ts --fix",
    "serve-debug": "tsnd --debug build/server.js",
    "start": "node build/server.js",
    "style-ck": "prettier --check --ext .ts",
    "style-fx": "prettier --write --ext .ts",
    "test": "jest --forceExit --coverage --verbose",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yarn watch-ts\" \"yarn serve-debug\"",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "aws-sdk": "2.1119.0",
    "bcrypt": "5.0.1",
    "body-parser": "1.20.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "date-fns": "2.28.0",
    "dotenv": "16.0.0",
    "express": "next",
    "express-validator": "6.14.0",
    "jsonwebtoken": "8.5.1",
    "uuid": "8.3.2",
    "zod": "3.14.4"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/body-parser": "1.19.2",
    "@types/config": "0.0.41",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.12",
    "@types/eslint": "8.4.1",
    "@types/eslint-config-prettier": "6.11.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "17.0.25",
    "@types/prettier": "2.6.0",
    "@types/rimraf": "3.0.2",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "jest": "27.5.1",
    "lerna": "4.0.0",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "supertest": "6.2.2",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.6.3"
  }
}

