{
  "name": "registration-validator-server",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Alfred Melson",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alfredmelson/mygomtech_server.git"
  },
  "main": "src/server.ts",
  "scripts": {
    "build-ts": "tsc",
    "build": "yarn build-ts && yarn lint-fx",
    "clean-svge": "rimraf coverage",
    "clean-dist": "rimraf dist",
    "clean-mdls": "lerna clean --yes && rimraf node_modules yarn.lock",
    "debug": "yarn build && yarn watch-debug",
    "dev": "tsnd --respawn --transpile-only src/server.ts",
    "lint-ck": "eslint ./src --ext .ts",
    "lint-fx": "eslint ./src --ext .ts --fix",
    "serve-debug": "tsnd --debug dist/server.js",
    "start": "node dist/src/server.js",
    "style-ck": "prettier --check ./src --ext .ts",
    "style-fx": "prettier --write ./src --ext .ts",
    "test": "jest --forceExit --coverage --verbose",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yarn watch-ts\" \"yarn serve-debug\"",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "1.19.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "express": "next",
    "express-validator": "^6.14.0",
    "jsonwebtoken": "^8.5.1",
    "uuid": "^8.3.2",
    "zod": "^3.12.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "1.19.2",
    "@types/config": "^0.0.41",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.1",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.21",
    "@types/prettier": "^2.4.4",
    "@types/rimraf": "^3.0.2",
    "@types/supertest": "2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "jest": "^27.5.1",
    "lerna": "^4.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "supertest": "6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  }
}
